[user]
	email = 423821+quinnjr@users.noreply.github.com
	name = Joseph R. Quinn
[http "https://gopkg.in"]
	followRedirects = true
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/bin/nvim
[credential]
	helper = store
[tag]
	forceSignAnnotated = true
[code]
	editor = /usr/bin/nvim
[pull]
	rebase = false
[safe]
	directory = /opt/flutter
[init]
	defaultBranch = main
[alias]
	lb = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
[push]
	autoSetupRemote = true
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
